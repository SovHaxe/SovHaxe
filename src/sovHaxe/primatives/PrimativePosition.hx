package sovHaxe.primatives;

/**
* Interface for the position of a visible element Primative (I.E The base level position resolved relative.
**/
class PrimativePosition {

    public var x:Int;
    public var y:Int;
    public var z:Int;

    public function new(x, y, z)
    {
            this.x = x;
            this.y = y;
            this.z = z;
    }


}
